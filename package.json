{
  "name": "ws-tictactoe",
  "version": "0.6.0",
  "private": true,
  "scripts": {
    "start": "node ./bin/www",
    "build": "cd client && npm run build",
    "heroku-postbuild": "cd client && npm install --production=false && npm run build",
    "dev": "concurrently \"npm run server:dev\" \"npm run client:dev\"",
    "server:dev": "nodemon ./bin/www",
    "client:dev": "cd client && npm run serve",
    "postinstall": "cd client && npm install"
  },
  "dependencies": {
    "bcrypt": "^5.0.1",
    "cookie-parser": "^1.4.6",
    "debug": "~2.6.9",
    "dotenv": "^16.0.1",
    "express": "~4.16.1",
    "express-ws": "^5.0.2",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^6.5.0",
    "morgan": "~1.9.1",
    "nanoid": "^3.1.30",
    "ws": "^8.4.0"
  },
  "devDependencies": {
    "concurrently": "^7.2.1",
    "nodemon": "^2.0.15"
  }
}
