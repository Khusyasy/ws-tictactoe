{
  "name": "ws-tictactoe",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "postinstall": "cd client && npm i",
    "start": "node ./bin/www",
    "build": "cd client && npm run build",
    "dev": "concurrently \"npm run server:dev\" \"npm run client:dev\"",
    "server:dev": "nodemon ./bin/www",
    "client:dev": "cd client && npm run serve"
  },
  "dependencies": {
    "cookie-parser": "~1.4.4",
    "debug": "~2.6.9",
    "ejs": "^3.1.6",
    "express": "~4.16.1",
    "express-ws": "^5.0.2",
    "morgan": "~1.9.1",
    "nanoid": "^3.1.30",
    "ws": "^8.4.0"
  },
  "devDependencies": {
    "concurrently": "^7.2.1",
    "nodemon": "^2.0.15"
  }
}
